mixin sComparation()
	// start sComparation
	+b.SECTION.sComparation#sComparation&attributes(attributes)
		+e.calc-wanna-fly
			img.lazy(src="#" data-src="img/@2x/calculator.png" alt="123")
			//-.lazy(src="#" data-
		.container
			+b.section-title
				h2 Сравнительная таблица бренда SUPERLOK c другими производителями
			+e.table-cont.table-responsive
				+e.TABLE.char-table.table
					+e.THEAD.table-header
						tr
							th.brend-col(scope="col")
								+e.header-cel-content
									.icon-cont
										+icon('comparation_cartwheel')
									.header-col-txt Бренд
							th.quality-col(scope="col")
								+e.header-cel-content
									.icon-cont
										+icon('comparation_diamond')
									.header-col-txt Качество продукции
							th.replacement-col(scope="col")
								+e.header-cel-content
									.icon-cont
										+icon('comparation_refresh')
									.header-col-txt Взаимозаменяемость
							th.delivery-time-col(scope="col")
								+e.header-cel-content
									.icon-cont
										+icon('comparation_sandClock')
									.header-col-txt Срок поставки
							th.price-col(scope="col")
								+e.header-cel-content
									.icon-cont
										+icon('comparation_coins')
									.header-col-txt Цена
					+e.TBODY.table-body
						tr
							+tbodyTHBrend(1)
							+tbodyTH('quality-col', 'Аналогично данному производителю')
								+icon('comparation_analog')
