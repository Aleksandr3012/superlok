mixin tbodyTHBrend(imgNum)
	th.brend-col
		+e.th-img-cont
			img.lazy(src="#" data-src=`img/@2x/table-comparation-${imgNum}.png` alt="")

mixin tbodyTH(ColClass='quality-col', ColTxt="ColTxt", hasToolTip=false)
	th(class=ColClass)
		+e.tbody-th-cont
			+e.tbody-icon-cont
				block
			+e.tbody-th-txt=ColTxt
			if hasToolTip
				.tooltip Сроки поставки от SUPERLOK значительно меньше, и экономит Ваше время