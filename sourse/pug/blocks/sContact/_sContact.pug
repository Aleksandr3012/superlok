mixin sContact()
	// start sContact
	+b.SECTION.sContact#sContact&attributes(attributes)
		.container
			+b.section-title.text-center
				h2 Наши контакты
		+e.contact-items-box
			.container
				+e.contacts-items
					+e.A.phone-item.contacts-item(href="tel:+")
						+e.icon-cont
							img.lazy(src="#" data-src="img/contacts_phone.svg" alt="")
						span.item-txt 8 (831) 423-86-95
					+e.A.mail-item.contacts-item(href="mailto:info@promsistemnn.ru")
						+e.icon-cont
							img.lazy(src="#" data-src="img/contacts_mail.svg" alt="")
						span.item-txt info@promsistemnn.ru
					+e.A.location-item.contacts-item(href="#")
						+e.icon-cont
							img.lazy(src="#" data-src="img/contacts_location.svg" alt="")
						span.item-txt 603011, г. Нижний Новгород, ул. Усиевича, д. 4а
		+e.map-cont
			iframe(src='https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d4429.306427867306!2d43.9337457234474!3d56.2840362124159!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x414e2a79dc7169d3%3A0xed1df6dc6dcd3505!2z0YPQuy4g0KPRgdC40LXQstC40YfQsCwgNNCQLCDQndC40LbQvdC40Lkg0J3QvtCy0LPQvtGA0L7QtCwg0J3QuNC20LXQs9C-0YDQvtC00YHQutCw0Y8g0L7QsdC7Liwg0KDQvtGB0YHQuNGPLCA2MDMwNzY!5e0!3m2!1sru!2sua!4v1592658459163!5m2!1sru!2sua' width='800' height='480' frameborder='0' style='border:0;' allowfullscreen='' aria-hidden='false' tabindex='0')
			+e.map-tooltip
				+e.map-tt-header Наш офис:
				+e.map-tt-txt 603011, г. Нижний Новгород, ул. Усиевича, д. 4а
	// end sContact